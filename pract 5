#include <stdio.h>

#define MOD 12345
#define MAX_N 10000

int main() {
    int n;
    scanf("%d", &n);

    int dp[MAX_N + 1] = {0};

    dp[0] = 1; 
    dp[1] = 2;  
    dp[2] = 4; 

    for (int i = 3; i <= n; i++) {
        dp[i] = (dp[i-1] + dp[i-2] + dp[i-3]) % MOD;
    }

    printf("%d\n", dp[n]);
    return 0;
}

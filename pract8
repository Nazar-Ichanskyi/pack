#include <stdio.h>
#include <string.h>

#define MAX_LEN 15
unsigned long long factorial(int n) {
    unsigned long long res = 1;
    for (int i = 2; i <= n; i++)
        res *= i;
    return res;
}

int main() {
    char word[MAX_LEN];
    scanf("%s", word);

    int freq[26] = {0}; // для літер A-Z

    int n = strlen(word);

    for (int i = 0; i < n; i++) {
        char ch = word[i];
        if (ch >= 'a' && ch <= 'z') ch -= 32;
        freq[ch - 'A']++;
    }

    unsigned long long total = factorial(n);
    for (int i = 0; i < 26; i++) {
        if (freq[i] > 1) {
            total /= factorial(freq[i]);
        }
    }

    printf("%llu\n", total);

    return 0;
}
